<script type="text/html" id="general-notification-template">
    <a class="notice-offers__item notice-offers__item_alert"
       data-bind="css: {
                    'notice-offers__item_visible': $data.visible(),
                    'notice-offers__item_success': $data.type === 'positive',
                    'notice-offers__item_warning': $data.type === 'warning',
                    'notice-offers__item_error': $data.type === 'negative',
                    'notice-offers__item_info': $data.type === 'general',
                    'notice-offers__item_linkless': !notification.url
                  },
                  attr: { href: $data.url },
                  event: { 'mouseover': $root.onNotificationMouseover.bind($root, $data),
                           'mouseout': $root.onNotificationMouseout.bind($root, $data),
                           'click': $root.onNotificationClick.bind($root) }">
        <div class="notice-offers__close"
             data-bind="click: $root.onNotificationCloseClick.bind($root, $index)"></div>
        <div class="notice-offers__details">
            <div class="notice-offers__details-list">
                <div class="notice-offers__details-item"
                     data-bind="text: $data.project"></div>
            </div>
        </div>
        <div class="notice-offers__description"
             data-bind="html: window.twemoji.parse($data.message)"></div>
    </a>
</script>

